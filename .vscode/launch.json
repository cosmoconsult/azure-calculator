    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Deno with sample config",
            "request": "launch",
            "type": "node",
            "program": "mod.ts",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "/deno/bin/deno",
            "runtimeArgs": [
                "run",
                "--inspect-brk",
                "--allow-all"
            ],
            "outputCapture": "std",
            "args": [
                "--config",
                "./configs/sample.json"
            ]
        },
        {
